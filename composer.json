{
    "name": "miw_upm/miw_api_users_results",
    "description": "MiW REST api",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "homepage": "https://miw.etsisi.upm.es/",
    "keywords": [
        "symfony",
        "doctrine",
        "rest api"
    ],
    "authors": [
        {
            "name": "MiW UPM",
            "homepage": "https://miw.etsisi.upm.es/",
            "email": "miw.etsisi@upm.es",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-sodium": "*",
        "doctrine/dbal": "^4.4.1",
        "doctrine/deprecations": "^1.1.5",
        "doctrine/doctrine-bundle": "^2.18.2",
        "doctrine/orm": "^3.6.0",
        "jms/serializer-bundle": "^5.5.2",
        "lexik/jwt-authentication-bundle": "^3.2.0",
        "symfony/console": "7.4.*",
        "symfony/dotenv": "7.4.*",
        "symfony/expression-language": "7.4.*",
        "symfony/flex": "^2.10.0",
        "symfony/framework-bundle": "7.4.*",
        "symfony/runtime": "7.4.*",
        "symfony/security-bundle": "7.4.*",
        "symfony/yaml": "7.3.*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24.1",
        "infection/infection": "^0.29.14",
        "jetbrains/phpstorm-attributes": "^1.2",
        "phpmetrics/phpmetrics": "v3.0.0rc6",
        "phpstan/phpstan": "^2.1.33",
        "phpunit/phpunit": "^10.5.60",
        "rector/rector": "^2.3.0",
        "symfony/browser-kit": "7.4.*",
        "symfony/css-selector": "7.4.*",
        "symfony/maker-bundle": "^1.65.1",
        "symfony/monolog-bundle": "^3.11.1",
        "symfony/phpunit-bridge": "7.4.*",
        "symfony/stopwatch": "7.4.*",
        "symfony/web-profiler-bundle": "7.4.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "infection/extension-installer": true
        },
        "bump-after-update": true,
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "infection": "./vendor/bin/infection --ansi --coverage=\"var/coverage\" --threads=20 --only-covered",
        "metrics": "vendor/bin/phpmetrics --junit=\"var/coverage/index.junit.xml\" --report-html=\"docs/PhpMetrics_report\" ./src",
        "phpstan": "./vendor/bin/phpstan --ansi"
    },
    "scripts-descriptions": {
        "infection": "Runs the mutation testing.",
        "metrics": "PhpMetrics provides metrics about PHP project and classes.",
        "phpstan": "PHP Static Analysis"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "runtime": {
            "use_putenv": true
        },
        "symfony": {
            "allow-contrib": false,
            "require": "7.4.*",
            "docker": false
        }
    }
}
